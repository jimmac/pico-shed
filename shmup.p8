function draw_game()
	cls(0)
	fr+=1
	--starfield
	starfield()

	--player
	spr(pl.sp,pl.x,pl.y)
	--flame
	spr(flame,pl.x,pl.y+5)
	--shooting & bullet
	bang()
	if bl.life>0 then spr(16,bl.x,bl.y) end

 --menu
 menu()
end

function draw_start()
	cls(2)
	print("\14 shmup",45,41,1)
	print("\14 shmup",45,40,7)
	
	print("press any button\
    to start", 30,70,13)
end
