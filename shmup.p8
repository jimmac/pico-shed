function starfield()
  for i=1,#starsx do
   --flicker color
   local col
   if starss[i]>1.9 then
   	col=7
   elseif starss[i]>1.7 then
    col=6
   else
    col=13
   end
   --render  
			if col==7 then
				line(starsx[i],starsy[i],
				     starsx[i],starsy[i]-1,
				     col)
			else
				pset(starsx[i],starsy[i],col)
			end
			--movement
			if (starsy[i]<128) then
				starsy[i]+=starss[i]
			else
				starsy[i]=0
			end
		end
end

function menu ()
  --score
  print("score: "..score,50,1,6)
  --lives
  for i=0,4 do --5 lives
	  if i<pl.lv then h=9 else h=8 end
	  spr(h,i*6)
  end
end
