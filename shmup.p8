function update_game()
	--flame
	flame=4+rnd(3)

 --ship movement
	if btn(â¡ï¸) and pl.x<119 then
		pl.spx=2
		pl.spy=0
		pl.sp=3
	elseif btn(â¬…ï¸) and pl.x>1 then
		pl.spx=-2
		pl.spy=0
		pl.sp=1
	elseif btn(â¬†ï¸) and pl.y>0 then
	 pl.sp=2
	 pl.spx=0
	 pl.spy=-1
	elseif btn(â¬‡ï¸) and pl.y>=0 and pl.y<110 then
	 pl.sp=2
	 pl.spx=0
	 pl.spy=2
	 flame=7 --small flame
	else 
	 pl.spy=0
		pl.spx=0	
		pl.sp=2
	end
	pl.x=pl.x+pl.spx
	pl.y=pl.y+pl.spy 
	
	--shooting
	if btnp(â) then
		sfx(0)
		bl.life=30
		bl.x=pl.x
		bl.y=pl.y-3
		flash=3
	end
	if bl.y>0 then
		bl.y-=5
	end
	bl.life-=1
end


function update_start()
	if btnp(â) or btnp(ğŸ…¾ï¸) then
		mode="game"
	end
end
