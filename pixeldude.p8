pico-8 cartridge // http://www.pico-8.com
version 41
__lua__
function _init ()
	--pink is tranparent
	palt(14,true)
	palt(0,false)
	pause=16
	fr=0
	--player.state--
	--	 0 - idle, 
	--	 1 - walk, 
	--	 2 - pull
	pl = {x=63,y=77,fr=0,state=0}
end

function _update60()
	if btn(➡️) then	
		pl.x+=1
		pl.state = 1
		r=true
 elseif btn(⬅️) then	
 	pl.x-=1
 	pl.state = 1
 	r=false
 else
 	pl.state = 0
 	r=false
 end
 --printh(pl.state)
 --loop the two sprites every `pause` frames
 if (fr<pause) then
   fr+=1
 else
   if (pl.fr==0) then 
   	pl.fr=1 
   else 
   	pl.fr=0 
   end
   fr=0
 end
end

function _draw()
 cls()
 map()
 spr(pl.state*3+3+pl.fr,
     pl.x,
     pl.y,1,1,r)
end
